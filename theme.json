{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"color": {
			"custom": true,
			"palette": [
				{
					"color": "#ddd",
					"slug": "off-white",
					"name": "Off-White"
				},
				{
					"color": "#333",
					"slug": "off-black",
					"name": "Off-Black"
				},
				{
					"color": "rgba(221,221,221,0.5)",
					"slug": "trans-off-white",
					"name": "Translucent Off-White"
				},
				{
					"color": "rgba(51,51,51,0.5)",
					"slug": "trans-off-black",
					"name": "Translucent Off-Black"
				},
				{
					"color": "#feba05",
					"slug": "primary-yellow",
					"name": "Primary Yellow"
				},
				{
					"color": "#fca102",
					"slug": "secondary-yellow",
					"name": "Secondary Yellow"
				},
				{
					"color": "#ef8000",
					"slug": "tertiary-yellow",
					"name": "Tertiary Yellow"
				},
				{
					"color": "#fa0c08",
					"slug": "primary-red",
					"name": "Primary Red"
				},
				{
					"color": "#e00001",
					"slug": "secondary-red",
					"name": "Secondary Red"
				},
				{
					"color": "#c80004",
					"slug": "tertiary-red",
					"name": "Tertiary Red"
				},
				{
					"color": "#005eff",
					"slug": "primary-blue",
					"name": "Primary Blue"
				},
				{
					"color": "#1fff80",
					"slug": "secondary-blue",
					"name": "Secondary Blue"
				}
			]
		},
		"layout": {
			"contentSize": "80%"
		},
		"spacing": {
			"blockGap": false
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Kinfolk",
					"name": "Kinfolk",
					"slug": "kinfolk",
					"fontFace": [
						{
							"fontFamily": "Kinfolk",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": ["./assets/fonts/Kinfolk.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "x-small",
					"size": "0.5rem",
					"name": "Extra Small"
				},
				{
					"slug": "small",
					"size": "0.8rem",
					"name": "Small"
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "Extra Large"
				},
				{
					"slug": "xx-large",
					"size": "3rem",
					"name": "Double XL"
				}
			]
		}
	},
	"styles": {
		"elements": {
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--primary-red)"
				}
			},
			"h2": {
				"color": {
					"text": "var(--wp--preset--color--secondary-red)"
				}
			},
			"h3": {
				"color": {
					"text": "var(--wp--preset--color--tertiary-red)"
				}
			},
			"h4": {
				"color": {
					"text": "var(--wp--preset--color--primary-yellow)"
				}
			},
			"h5": {
				"color": {
					"text": "var(--wp--preset--color--secondary-yellow)"
				}
			},
			"h6": {
				"color": {
					"text": "var(--wp--preset--color--tertiary-yellow)"
				}
			}
		},
		"typography": {
			"fontFamily": "Kinfolk"
		}
	}
}